{
  "name": "ai-legal-agent",
  "version": "0.1.0",
  "description": "AI法务智能体系统 - 基于多智能体协作的超级AI法务系统",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn src.api.main:app --reload --host 0.0.0.0 --port 8001",
    "build": "cd frontend && npm run build",
    "setup": "npm install && cd frontend && npm install",
    "setup:backend": "cd backend && uv sync",
    "setup:all": "npm run setup && npm run setup:backend",
    "start": "npm run dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-legal-agent"
  },
  "keywords": [
    "ai",
    "legal",
    "multi-agent",
    "camel-ai",
    "law",
    "contract-review",
    "due-diligence"
  ],
  "author": "AI Legal Agent Team",
  "license": "Apache-2.0"
}
